				<key>public.filename-extension</key>
				<array>
					<string>encvideo</string>
					<string>encimage</string>
				</array>
				<key>public.mime-type</key>
				<array>
					<string>application/x-encimage</string>
				</array>
			</dict>
		</dict>
	</array>
